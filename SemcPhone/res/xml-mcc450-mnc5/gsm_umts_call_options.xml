    <CheckBoxPreference android:persistent="true" android:title="@string/international_call_lock_title" android:key="checkbox_international_calllock_key" android:defaultValue="false" android:summaryOn="@string/international_call_lock_enabled" android:summaryOff="@string/international_call_lock_disabled" />
